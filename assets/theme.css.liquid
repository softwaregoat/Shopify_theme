.countdown__items {
  display: grid;
  position: relative;
  grid-template-columns: repeat(4, 1fr);
  width: 100%;
  min-width: -moz-fit-content;
  min-width: fit-content;
  gap: var(--countdown-item-gap);
  padding: var(--countdown-item-padding);
  border: 1px solid var(--text-color-alpha);
}
